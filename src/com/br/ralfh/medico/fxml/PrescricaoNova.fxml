<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane styleClass="fundo" stylesheets="@../css/prescricao.css" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.br.ralfh.medico.PrescricaoNovaController">
   <children>
      <HBox layoutX="242.0" layoutY="120.0" spacing="15.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox spacing="10.0">
               <children>
                  <VBox spacing="4.0">
                     <children>
                        <Label text="1 - Medicamento:">
                           <font>
                              <Font name="System Bold" size="12.0" />
                           </font></Label>
                        <HBox alignment="CENTER_LEFT" spacing="4.0" />
                     </children>
                  </VBox>
                  <TabPane fx:id="tPaneMedicamento" prefHeight="400.0" prefWidth="300.0" tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                        <Tab fx:id="tabPrincipio" text="Principio Ativo">
                           <content>
                              <AnchorPane minHeight="0.0" minWidth="0.0">
                                 <children>
                                    <HBox alignment="CENTER_LEFT" spacing="4.0" AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="10.0">
                                       <children>
                                          <TextField fx:id="editPrincipio" prefWidth="245.0" />
                                          <Button fx:id="btnApagaPrincipio" alignment="CENTER" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" onAction="#btnApagaPrincipioFired" prefHeight="25.0" prefWidth="25.0" styleClass="botaopq" textAlignment="CENTER">
                                             <graphic>
                                                <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" styleClass="botaopq">
                                                   <image>
                                                      <Image url="@../imagens/Draw-Eraser.png" />
                                                   </image>
                                                </ImageView>
                                             </graphic>
                                          </Button>
                                       </children>
                                    </HBox>
                                    <TableView fx:id="tabelaPrincipios" layoutX="7.0" layoutY="14.0" prefWidth="280.0" AnchorPane.bottomAnchor="2.0" AnchorPane.leftAnchor="2.0" AnchorPane.rightAnchor="2.0" AnchorPane.topAnchor="45.0">
                                       <columns>
                                          <TableColumn fx:id="colunaPrincipio" prefWidth="280.0" text="Nome" />
                                       </columns>
                                    </TableView>
                                 </children>
                              </AnchorPane>
                           </content>
                        </Tab>
                        <Tab fx:id="tabMedicamento" text="Nome Comercial">
                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                 <children>
                                    <HBox alignment="CENTER_LEFT" spacing="4.0" AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="10.0">
                                       <children>
                                          <TextField fx:id="editMedicamento" prefWidth="245.0" />
                                          <Button fx:id="btnApagaMedicamento" alignment="CENTER" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" onAction="#btnApagaMedicamentoFired" prefHeight="25.0" prefWidth="25.0" styleClass="botaopq" textAlignment="CENTER">
                                             <graphic>
                                                <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" styleClass="botaopq">
                                                   <image>
                                                      <Image url="@../imagens/Draw-Eraser.png" />
                                                   </image>
                                                </ImageView>
                                             </graphic>
                                          </Button>
                                       </children>
                                    </HBox>
                                    <TableView fx:id="tabelaMedicamentos" layoutX="7.0" layoutY="14.0" AnchorPane.bottomAnchor="2.0" AnchorPane.leftAnchor="2.0" AnchorPane.rightAnchor="2.0" AnchorPane.topAnchor="45.0">
                                      <columns>
                                        <TableColumn fx:id="colunaMedicamento" editable="false" prefWidth="280.0" sortable="false" text="Nome" />
                                      </columns>
                                    </TableView>
                                 </children>
                              </AnchorPane>
                          </content>
                        </Tab>
                    </tabs>
                  </TabPane>
                  <VBox spacing="4.0">
                     <children>
                        <Label text="Grupo" />
                        <ComboBox fx:id="cbGrupo" prefWidth="280.0" />
                     </children>
                  </VBox>
               </children>
            </VBox>
            <VBox layoutX="10.0" layoutY="10.0" spacing="10.0">
               <children>
                  <VBox spacing="10.0">
                     <children>
                        <Label text="2 - Posologia:">
                           <font>
                              <Font name="System Bold" size="12.0" />
                           </font></Label>
                        <HBox spacing="4.0">
                           <children>
                              <TextField fx:id="editPosologia" prefWidth="300.0" />
                              <Button fx:id="btnApagaChavePosologia" mnemonicParsing="false" onAction="#btnApagaChaveModoUsoClicked" prefWidth="25.0" styleClass="botaopq">
                                 <graphic>
                                    <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" styleClass="botaopq">
                                       <image>
                                          <Image url="@../imagens/Draw-Eraser.png" />
                                       </image>
                                    </ImageView>
                                 </graphic></Button>
                           </children>
                        </HBox>
                        <ListView fx:id="listaPosologias" prefHeight="200.0" />
                     </children>
                  </VBox>
                  <VBox>
                     <children>
                        <VBox spacing="10.0">
                           <children>
                              <Label text="3 - Via de administração:">
                                 <font>
                                    <Font name="System Bold" size="12.0" />
                                 </font>
                              </Label>
                              <HBox spacing="10.0">
                                 <children>
                                    <VBox spacing="5.0">
                                       <children>
                                          <RadioButton fx:id="rbUsoInterno" mnemonicParsing="false" text="Uso interno">
                                             <toggleGroup>
                                                <ToggleGroup fx:id="tgViaAdmin" />
                                             </toggleGroup>
                                          </RadioButton>
                                          <RadioButton fx:id="rbUsoExterno" mnemonicParsing="false" text="Uso externo" toggleGroup="$tgViaAdmin" />
                                          <RadioButton fx:id="rbUsoTopico" mnemonicParsing="false" text="Uso tópico" toggleGroup="$tgViaAdmin" />
                                       </children>
                                    </VBox>
                                    <VBox spacing="5.0">
                                       <children>
                                          <RadioButton fx:id="rbUsoViaOral" mnemonicParsing="false" text="Uso via oral" toggleGroup="$tgViaAdmin" />
                                          <RadioButton fx:id="rbUsoInjetavel" mnemonicParsing="false" text="Uso injetável" toggleGroup="$tgViaAdmin" />
                                          <RadioButton fx:id="rbUsoOutros" mnemonicParsing="false" text="Outros" toggleGroup="$tgViaAdmin" />
                                          <HBox spacing="4.0">
                                             <children>
                                                <TextField fx:id="editViaAdmin" prefWidth="170.0" />
                                                <Button fx:id="btnApagaChaveViaAdmin" mnemonicParsing="false" onAction="#btnApagaChaveAplicacaoClicked" prefWidth="25.0" styleClass="botaopq">
                                                   <graphic>
                                                      <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" styleClass="botaopq">
                                                         <image>
                                                            <Image url="@../imagens/Draw-Eraser.png" />
                                                         </image>
                                                      </ImageView>
                                                   </graphic>
                                                </Button>
                                             </children>
                                          </HBox>
                                       </children>
                                    </VBox>
                                 </children>
                              </HBox>
                           </children>
                           <VBox.margin>
                              <Insets left="5.0" top="5.0" />
                           </VBox.margin>
                        </VBox>
                        <VBox spacing="4.0">
                           <children>
                              <Label text="4 - Quantidade:">
                                 <font>
                                    <Font name="System Bold" size="12.0" />
                                 </font>
                              </Label>
                              <HBox spacing="4.0">
                                 <children>
                                    <TextField fx:id="editQuantidade" prefWidth="130.0" />
                                    <Button fx:id="btnApagaChaveQuantidade" mnemonicParsing="false" onAction="#btnApagaChaveQuantidadeClicked" prefWidth="25.0" styleClass="botaopq">
                                       <graphic>
                                          <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" styleClass="botaopq">
                                             <image>
                                                <Image url="@../imagens/Draw-Eraser.png" />
                                             </image>
                                          </ImageView>
                                       </graphic>
                                    </Button>
                                 </children>
                                 <padding>
                                    <Insets left="5.0" />
                                 </padding>
                              </HBox>
                           </children>
                        </VBox>
                     </children>
                  </VBox>
                  <HBox alignment="CENTER" spacing="10.0">
                     <children>
                        <Button fx:id="btnConfirma" mnemonicParsing="false" onAction="#actionConfirmar" prefHeight="30.0" prefWidth="92.0" styleClass="botaotool" text="Prescreve">
                           <graphic>
                              <ImageView fitHeight="20.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../imagens/accept.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                        <Button fx:id="btnCancela" layoutX="89.0" layoutY="10.0" mnemonicParsing="false" onAction="#actionCancelar" prefHeight="30.0" prefWidth="92.0" styleClass="botaotool" text="Cancela">
                           <graphic>
                              <ImageView fitHeight="20.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../imagens/Cancel-32.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                     </children>
                     <VBox.margin>
                        <Insets top="15.0" />
                     </VBox.margin>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</AnchorPane>
